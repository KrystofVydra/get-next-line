            <message component="qac" version="9.1.0" id="2500" mapped="yes"/>
          </enforcement>
        </rule>
        <rule id="M3CM Rule-22.9" help="m3cm-2_1_0_rule_help-en_US/Rule_Group_7_ID_22_9.html" active="yes">
          <text>The value of errno shall be tested against zero after calling an errno-setting-function</text>
          <categories>
            <category>MISRA Required Rules</category>
            <category>Required</category>
            <category>Resources</category>
            <category>Rule</category>
            <category>Rules</category>
          </categories>
          <enforcement>
            <message component="qac" version="9.1.0" id="2501" mapped="yes"/>
          </enforcement>
        </rule>
        <rule id="M3CM Rule-22.10" help="m3cm-2_1_0_rule_help-en_US/Rule_Group_7_ID_22_10.html" active="yes">
          <text>The value of errno shall only be tested when the last function to be called was an errno-setting-function</text>
