            <message component="qac" version="9.1.0" id="1486" mapped="yes"/>
          </enforcement>
        </rule>
        <rule id="M3CM Rule-22.6" help="m3cm-2_1_0_rule_help-en_US/Rule_Group_8_ID_22_6.html" active="yes">
          <text>The value of a pointer to a FILE shall not be used after the associated stream has been closed</text>
          <categories>
            <category>MISRA Mandatory Rules</category>
            <category>Mandatory</category>
            <category>Resources</category>
            <category>Rule</category>
            <category>Rules</category>
          </categories>
          <enforcement>
          </enforcement>
        </rule>
      </enforcement>
