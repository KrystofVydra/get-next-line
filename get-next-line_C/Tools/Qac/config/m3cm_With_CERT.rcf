        <rule id="M3CM Rule-21.14" help="m3cm-2_1_0_rule_help-en_US/Rule_Group_7_ID_21_14.html" active="yes">
          <text>The Standard Library function memcmp shall not be used to compare null terminated strings</text>
          <categories>
            <category>MISRA Required Rules</category>
            <category>Required</category>
            <category>Rule</category>
            <category>Rules</category>
            <category>Standard libraries</category>
          </categories>
          <enforcement>
          </enforcement>
        </rule>
        <rule id="M3CM Rule-21.15" help="m3cm-2_1_0_rule_help-en_US/Rule_Group_7_ID_21_15.html" active="yes">
          <text>The pointer arguments to the Standard Library functions memcpy, memmove and memcmp shall be pointers to qualified or unqualified versions of compatible types</text>
          <categories>
            <category>MISRA Required Rules</category>
            <category>Required</category>
            <category>Rule</category>
            <category>Rules</category>
            <category>Standard libraries</category>
          </categories>
          <enforcement>
            <message component="qac" version="9.1.0" id="1487" mapped="yes"/>
          </enforcement>
        </rule>
        <rule id="M3CM Rule-21.16" help="m3cm-2_1_0_rule_help-en_US/Rule_Group_7_ID_21_16.html" active="yes">
          <text>The pointer arguments to the Standard Library function memcp shall point to either a pointer type, an essentially signed type, an essentially unsigned type, an essentially Boolean type or an essentially enum type</text>
          <categories>
            <category>MISRA Required Rules</category>
            <category>Required</category>
            <category>Rule</category>
            <category>Rules</category>
            <category>Standard libraries</category>
          </categories>
          <enforcement>
            <message component="qac" version="9.1.0" id="1488" mapped="yes"/>
            <message component="qac" version="9.1.0" id="1489" mapped="yes"/>
            <message component="qac" version="9.1.0" id="1490" mapped="yes"/>
            <message component="qac" version="9.1.0" id="1491" mapped="yes"/>
          </enforcement>
        </rule>
        <rule id="M3CM Rule-22.1" help="m3cm-2_1_0_rule_help-en_US/Rule_Group_7_ID_22_1.html" active="yes">
          <text>All resources obtained dynamically by means of Standard Library functions shall be explicitly released</text>
          <categories>
            <category>MISRA Required Rules</category>
            <category>Required</category>
            <category>Resources</category>
            <category>Rule</category>
            <category>Rules</category>
          </categories>
          <enforcement>
          </enforcement>
        </rule>
        <rule id="M3CM Rule-22.3" help="m3cm-2_1_0_rule_help-en_US/Rule_Group_7_ID_22_3.html" active="yes">
          <text>The same file shall not be open for read and write access at the same time on different streams</text>
          <categories>
            <category>MISRA Required Rules</category>
            <category>Required</category>
            <category>Resources</category>
            <category>Rule</category>
            <category>Rules</category>
          </categories>
          <enforcement>
          </enforcement>
        </rule>
        <rule id="M3CM Rule-22.7" help="m3cm-2_1_0_rule_help-en_US/Rule_Group_7_ID_22_7.html" active="yes">
          <text>The macro EOF shall on ly be compared with the unmodified return value from any Standard Library function capable of returning EOF</text>
